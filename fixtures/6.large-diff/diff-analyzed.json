      "clause_tags": []
    },
    {
      "hierarchy_level": "first_level",
      "source_text": "第3条(競業避止義務)\n",
      "page_num": 1,
      "start_line_num": 13,
      "start_char_num": 395,
      "end_char_num": 406,
      "is_unreliable": false,
      "heading": {
        "title": "競業避止義務",
        "clause_index": 0,
        "is_unreliable": false
      },
      "identifier": {
        "label_text": "第3条",
        "label_type": "jou",
        "clause_index": 0,
