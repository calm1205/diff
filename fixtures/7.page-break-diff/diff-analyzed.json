    },
    {
      "hierarchy_level": "first_level",
      "source_text": "第2条(使用許諾)\n",
      "page_num": 1,
      "start_line_num": 8,
      "start_char_num": 244,
      "end_char_num": 253,
      "is_unreliable": false,
      "heading": {
