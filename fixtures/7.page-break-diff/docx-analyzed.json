    {
      "hierarchy_level": "first_level",
      "source_text": "第6条(使用状況の報告·記録)\n",
      "page_num": 1,
      "start_line_num": 28,
      "start_char_num": 814,
      "end_char_num": 829,
      "is_unreliable": false,
      "heading": {
