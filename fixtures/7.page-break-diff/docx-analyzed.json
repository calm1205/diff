    },
    {
      "hierarchy_level": "first_level",
      "source_text": "第4条(許諾された使用の対価)\n",
      "page_num": 1,
      "start_line_num": 19,
      "start_char_num": 570,
      "end_char_num": 585,
      "is_unreliable": false,
      "heading": {
