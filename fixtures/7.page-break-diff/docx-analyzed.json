    },
    {
      "hierarchy_level": "first_level",
      "source_text": "第9条(権利の保全)\n",
      "page_num": 2,
      "start_line_num": 44,
      "start_char_num": 1354,
      "end_char_num": 1364,
      "is_unreliable": false,
